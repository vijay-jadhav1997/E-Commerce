{
  "version": 2,
  "builds": [
    {
      "src": "requirements.txt",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "ecommerce/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  
  "routes": [
    {
      "src": "/(.*)",
      "dest": "ecommerce.wsgi:application"
    }
  ]
}
